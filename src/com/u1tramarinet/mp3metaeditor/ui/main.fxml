<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane minHeight="500.0" minWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.u1tramarinet.mp3metaeditor.ui.Controller">
    <top>
        <ButtonBar prefHeight="40.0" prefWidth="200.0">
            <buttons>
            <ToggleButton mnemonicParsing="false" text="ToggleButton" />
                <Button fx:id="fileButton" mnemonicParsing="false" text="File" />
                <Button fx:id="resetButton" mnemonicParsing="false" text="Reset" />
            </buttons>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </ButtonBar>
    </top>
    <center>
        <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <content>
                <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="180.0" prefWidth="440.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="160.0" minWidth="10.0" prefWidth="135.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="10.0" prefWidth="195.0" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Label alignment="CENTER" text="ファイル名">
                                    <padding>
                                        <Insets left="20.0" right="11.0" top="3.0" />
                                    </padding>
                                </Label>
                                <Label text="曲名" GridPane.rowIndex="1">
                                    <padding>
                                        <Insets left="20.0" right="33.0" top="3.0" />
                                    </padding>
                                </Label>
                                <Label text="アーティスト" GridPane.rowIndex="2">
                                    <padding>
                                        <Insets left="20.0" right="11.0" top="3.0" />
                                    </padding>
                                </Label>
                                <Label text="トラック番号" GridPane.rowIndex="3">
                                    <padding>
                                        <Insets left="20.0" right="5.0" top="3.0" />
                                    </padding>
                                </Label>
                                <Label text="アルバムアーティスト" GridPane.rowIndex="4">
                                    <padding>
                                        <Insets left="20.0" top="3.0" />
                                    </padding>
                                </Label>
                                <Label text="アルバム" GridPane.rowIndex="5">
                                    <padding>
                                        <Insets left="20.0" top="3.0" />
                                    </padding>
                                </Label>
                                <TextField fx:id="fileName" editable="false" promptText="file name" GridPane.columnIndex="1" />
                                <TextField fx:id="trackName" promptText="track name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                <TextField fx:id="artistName" promptText="artist name" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                <TextField fx:id="trackNumber" promptText="track number" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                <TextField fx:id="albumArtistName" promptText="album artist name" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                                <TextField fx:id="albumName" promptText="album name" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                                <Button fx:id="artistNameFullSetButton" mnemonicParsing="false" text="リスト一括設定" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
                                <Button fx:id="trackNumberFullSetButton" mnemonicParsing="false" text="リスト昇順採番" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER" />
                                <Button fx:id="albumArtistNameFullSetButton" mnemonicParsing="false" text="リスト一括設定" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
                                <Button fx:id="albumNameFullSetButton" mnemonicParsing="false" text="リスト一括設定" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="5" GridPane.valignment="CENTER" />
                            </children>
                            <VBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                        </GridPane>
                        <Button fx:id="trackSetButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="設定">
                            <VBox.margin>
                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                            </VBox.margin>
                        </Button>
                    </children>
                </VBox>
            </content>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </ScrollPane>
    </center>
    <left>
        <ScrollPane hbarPolicy="NEVER" prefHeight="460.0" prefWidth="232.0" BorderPane.alignment="CENTER">
            <content>
                <ListView fx:id="fileListView" prefHeight="200.0" prefWidth="230.0" />
            </content>
        </ScrollPane>
    </left>
</BorderPane>
