<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxWidth="1.7976931348623157E308" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.u1tramarinet.mp3metaeditor.java.ui.UnitSetController">
    <children>
        <GridPane alignment="TOP_CENTER" hgap="20.0" maxWidth="1.7976931348623157E308" minWidth="-Infinity" vgap="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" minWidth="-Infinity"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" percentWidth="79.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label fx:id="fileNameLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" text="ファイル名"/>
                <Label fx:id="trackLabel" alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308"
                       text="曲名" textAlignment="CENTER" GridPane.rowIndex="1"/>
                <Label fx:id="artistLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" text="アーティスト"
                       GridPane.rowIndex="2"/>
                <Label fx:id="trackNumberLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" text="トラック番号"
                       GridPane.rowIndex="3"/>
                <Label fx:id="albumArtistLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" text="アルバムアーティスト"
                       GridPane.rowIndex="4"/>
                <Label fx:id="albumLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" text="アルバム"
                       GridPane.rowIndex="5"/>
                <TextField fx:id="fileName" editable="false" promptText="file name" GridPane.columnIndex="1"/>
                <TextField fx:id="trackName" promptText="track name" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="artistName" promptText="artist name" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <TextField fx:id="trackNumber" promptText="track number" GridPane.columnIndex="1"
                           GridPane.rowIndex="3"/>
                <TextField fx:id="albumArtistName" promptText="album's artist name" GridPane.columnIndex="1"
                           GridPane.rowIndex="4"/>
                <TextField fx:id="albumName" promptText="album name" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
            </children>
            <VBox.margin>
                <Insets/>
            </VBox.margin>
        </GridPane>
        <HBox alignment="BASELINE_CENTER" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="200.0"
              spacing="20.0">
            <children>
                <Button fx:id="undoButton" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" text="元に戻す"
                        HBox.hgrow="ALWAYS"/>
                <Button fx:id="unitSettingButton" maxWidth="1.7976931348623157E308" minWidth="-Infinity"
                        mnemonicParsing="false" text="設定" HBox.hgrow="ALWAYS"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
